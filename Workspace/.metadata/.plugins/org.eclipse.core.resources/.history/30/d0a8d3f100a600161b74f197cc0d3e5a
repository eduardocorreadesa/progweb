package br.com.servlet;

import java.io.IOException;

import javax.servlet.RequestDispatcher;
import javax.servlet.ServletException;
import javax.servlet.annotation.WebServlet;
import javax.servlet.http.HttpServlet;
import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

@WebServlet("/pessoaCadastrada")
public class PessoaCadastrada extends HttpServlet {

	protected void doGet(HttpServletRequest request,
			HttpServletResponse response)
					throws IOException, ServletException {

//		String message = "dudu";
//		request.setAttribute("message", message);
//		request.getRequestDispatcher("pessoaCadastrada.jsp").forward(request,response);
		

		request.setAttribute("nome","value=dudu");
		System.out.println("teste "+request.getParameter("nome"));
		RequestDispatcher dispatcher = request.getRequestDispatcher("pessoaCadastrada.jsp");
		dispatcher.forward(request,response);

	}

}
